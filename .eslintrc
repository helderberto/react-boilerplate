{
  "extends": ["plugin:react/recommended", "airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "babel"],
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": ["dist/**"] }]
  }
}
